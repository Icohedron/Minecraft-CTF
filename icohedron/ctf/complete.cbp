//#-noreset

:install PULSE 16 0 1
// Info
// Entities used
// xp_orb, armor_stand, fireworks_rocket, item, arrow, area_effect_cloud
// 1.10 versions
// XPOrb, ArmorStand, FireworksRocketEntity, Item, Arrow, AreaEffectCloud

// Summon armor stands
/minecraft:summon ArmorStand ~ ~0.0 ~ {CustomName:"CTF Game",Tags:["CTFGame"],Invulnerable:1,Marker:1,NoGravity:1}
/minecraft:summon ArmorStand ~ ~0.1 ~ {CustomName:"CTF Red Marker",Tags:["CTFRedMarker","CTFTeamMarker"],Invulnerable:1,Marker:1,NoGravity:1}
/minecraft:summon ArmorStand ~ ~0.2 ~ {CustomName:"CTF Blue Marker",Tags:["CTFBlueMarker","CTFTeamMarker"],Invulnerable:1,Marker:1,NoGravity:1}
/minecraft:summon ArmorStand ~ ~0.3 ~ {CustomName:"CTF Team Balancer",Tags:["CTFTeamBalancer"],Invulnerable:1,Marker:1,NoGravity:1}
/minecraft:summon ArmorStand ~ ~0.4 ~ {CustomName:"CTF Existance Checker",Tags:["CTFExists"],Invulnerable:1,Marker:1,NoGravity:1}

// Scoreboard objectives
/minecraft:scoreboard objectives add CTFGameState dummy Game State
/minecraft:scoreboard objectives add CTFActive dummy Active
/minecraft:scoreboard objectives add CTFScore dummy Score
/minecraft:scoreboard objectives add CTFScoreKeeper dummy Score Keeper
/minecraft:scoreboard objectives add CTFTeamCounter dummy Team Count
/minecraft:scoreboard objectives add CTFRoundTime dummy Round Time
/minecraft:scoreboard objectives add CTFExists dummy Existance
/minecraft:scoreboard objectives add CTFTrigger trigger Context Trigger
/minecraft:scoreboard objectives add CTFSpectate trigger Spectate
/minecraft:scoreboard objectives add CTFDeathCount deathCount Death Count
/minecraft:scoreboard objectives add CTFDisconnect stat.leaveGame Disconnect Counter

// Entities with CTFFlagTracker == 1 are the holder of a flag
// Exception is for the armor stands CTFRed and CTFBlue which count score
/minecraft:scoreboard objectives add CTFFlagTracker dummy Flag Tracker

// Teams
/minecraft:scoreboard teams add CTFRed Red Team
/minecraft:scoreboard teams option CTFRed color red
/minecraft:scoreboard teams option CTFRed friendlyfire false
/minecraft:scoreboard teams option CTFRed nametagVisibility hideForOtherTeams

/minecraft:scoreboard teams add CTFBlue Blue Team
/minecraft:scoreboard teams option CTFBlue color blue
/minecraft:scoreboard teams option CTFBlue friendlyfire false
/minecraft:scoreboard teams option CTFBlue nametagVisibility hideForOtherTeams

// Teams not for players
/minecraft:scoreboard teams add CTFVRed Red Team
/minecraft:scoreboard teams option CTFVRed color red
/minecraft:scoreboard teams add CTFVBlue Blue Team
/minecraft:scoreboard teams option CTFVBlue color blue
/minecraft:scoreboard teams join CTFVRed Red
/minecraft:scoreboard teams join CTFVBlue Blue
/minecraft:scoreboard teams join CTFVRed Red_Score
/minecraft:scoreboard teams join CTFVBlue Blue_Score
/minecraft:scoreboard teams join CTFVRed Red_Players
/minecraft:scoreboard teams join CTFVBlue Blue_Players

// Set constants and defaults
/minecraft:scoreboard players set Red CTFScore 0
/minecraft:scoreboard players set Blue CTFScore 0
/minecraft:scoreboard players set TicksPerSecond CTFRoundTime 20
/minecraft:scoreboard players set TicksPerMinute CTFRoundTime 1200
/minecraft:scoreboard players set @e[type=ArmorStand,tag=CTFExists] CTFExists 0

/minecraft:scoreboard players set TimeInTicks CTFRoundTime 12000
/minecraft:scoreboard players set Winning_Score CTFScore 3

/minecraft:scoreboard players operation @e[type=ArmorStand,tag=CTFGame] CTFRoundTime = TimeInTicks CTFRoundTime
/minecraft:scoreboard objectives setdisplay sidebar CTFScore

// Initial game state
/minecraft:scoreboard players set @e[type=ArmorStand,tag=CTFGame] CTFGameState 0

// Turn off natural regeneration
/minecraft:gamerule naturalRegeneration false

// Configure event server spawn point system
/minecraft:scoreboard players set @e[type=ArmorStand,tag=SpawnStand] SPtp 33
/minecraft:scoreboard players set @e[type=ArmorStand,tag=SpawnStand] SPsat 0
/minecraft:scoreboard players set @e[type=ArmorStand,tag=SpawnStand] SPpvp 1

:uninstall PULSE 16 0 1
// Teleport all players back to spawn and clear inventories
/minecraft:execute @a[score_CTFActive_min=1,score_CTFActive=1] ~ ~ ~ minecraft:tp @p @r[score_CTFActive_min=1,type=ArmorStand,tag=CTFLobbySpawnPoint]
/minecraft:execute @a[tag=CTFSpectator] ~ ~ ~ minecraft:tp @p @r[score_CTFActive_min=1,type=ArmorStand,tag=CTFLobbySpawnPoint]
/minecraft:clear @a[score_CTFActive_min=1,score_CTFActive=1]

// Gets rid of all scoreboard objectives, entities, and resets gamerules to vanilla defaults
/minecraft:kill @e[type=ArmorStand,tag=CTFGame]
/minecraft:kill @e[type=ArmorStand,tag=CTFRedMarker]
/minecraft:kill @e[type=ArmorStand,tag=CTFBlueMarker]
/minecraft:kill @e[type=ArmorStand,tag=CTFTeamBalancer]
/minecraft:kill @e[type=ArmorStand,tag=CTFExists]
/minecraft:kill @e[type=Item,tag=CTFFlag]
/minecraft:kill @e[type=AreaEffectCloud,tag=CTFStartTimer]
/minecraft:scoreboard objectives remove CTFGameState
/minecraft:scoreboard objectives remove CTFActive
/minecraft:scoreboard objectives remove CTFScore
/minecraft:scoreboard objectives remove CTFScoreKeeper
/minecraft:scoreboard objectives remove CTFTeamCounter
/minecraft:scoreboard objectives remove CTFFlagTracker
/minecraft:scoreboard objectives remove CTFDeathCount
/minecraft:scoreboard objectives remove CTFDisconnect
/minecraft:scoreboard objectives remove CTFRoundTime
/minecraft:scoreboard objectives remove CTFTrigger
/minecraft:scoreboard objectives remove CTFSpectate
/minecraft:scoreboard objectives remove CTFExists
/minecraft:scoreboard teams remove CTFRed
/minecraft:scoreboard teams remove CTFBlue
/minecraft:scoreboard teams remove CTFEC
/minecraft:scoreboard teams remove CTFVRed
/minecraft:scoreboard teams remove CTFVBlue
/minecraft:gamerule naturalRegeneration true

// Configure event server spawn point system
/minecraft:scoreboard players set @e[type=ArmorStand,tag=SpawnStand] SPtp 0
/minecraft:scoreboard players set @e[type=ArmorStand,tag=SpawnStand] SPsat 1
/minecraft:scoreboard players set @e[type=ArmorStand,tag=SpawnStand] SPpvp 0

:main CLOCK 16 0 1
// Keep track of which entities are currently in the game
/minecraft:scoreboard players set @e CTFActive 0

// [User Input]
// The following assigns the score CTFActive to all entities within the map (including the lobby)
// Use an appropriate selector. Replace @e[r=-1]
/minecraft:scoreboard players set @e[x=535,y=0,z=129,dx=50,dy=256,dz=64,r=-1] CTFActive 1

// Exclude spectators
/minecraft:scoreboard players set @a[score_CTFActive_min=1,m=creative] CTFActive 2
/minecraft:scoreboard players set @a[score_CTFActive_min=1,m=spectator] CTFActive 2
/minecraft:scoreboard players set @a[score_CTFActive_min=1,score_CTFSpectate_min=1] CTFActive 2

// Spectators leave their team if they were on one
/minecraft:scoreboard teams leave @a[score_CTFActive_min=2,team=CTFRed]
/minecraft:scoreboard teams leave @a[score_CTFActive_min=2,team=CTFBlue]

// [User Input]
// Use event server spawn point system. Set spawn points to a random active lobby spawn point
// Use the assigned 'SPid' for the map -- currently using 33
scoreboard players add @a[score_CTFActive_min=1,score_CTFActive=1] SPid 0
execute @a[score_CTFActive_min=1,score_CTFActive=1,score_SPid=-34] ~ ~ ~ minecraft:tp @p @r[score_CTFActive_min=1,type=ArmorStand,tag=CTFLobbySpawnPoint]
execute @a[score_CTFActive_min=1,score_CTFActive=1,score_SPid_min=-32] ~ ~ ~ minecraft:tp @p @r[score_CTFActive_min=1,type=ArmorStand,tag=CTFLobbySpawnPoint]
scoreboard players set @a[score_CTFActive_min=1,score_CTFActive=1,score_SPid=-34] SPid 33
scoreboard players set @a[score_CTFActive_min=1,score_CTFActive=1,score_SPid_min=-32] SPid 33

// CTF Control Panel book given to Event Coordinators
// /give @p written_book 1 0 {pages:["[\"\",{\"text\":\"Capture the Flag\",\"bold\":true},{\"text\":\"\\n\",\"bold\":false},{\"text\":\"Game: \"},{\"text\":\"[Start]\",\"color\":\"dark_green\",\"clickEvent\":{\"action\":\"run_command\",\"value\":\"/trigger CTFTrigger set 1\"}},{\"text\":\"\\n\",\"color\":\"none\"},{\"text\":\"Teams: \",\"color\":\"none\"},{\"text\":\"\\n\",\"color\":\"none\"},{\"text\":\"[Add Red]\",\"color\":\"red\",\"clickEvent\":{\"action\":\"run_command\",\"value\":\"/trigger CTFTrigger set 6\"}},{\"text\":\" \",\"color\":\"none\"},{\"text\":\"[Add Blue]\",\"color\":\"blue\",\"clickEvent\":{\"action\":\"run_command\",\"value\":\"/trigger CTFTrigger set 7\"}},{\"text\":\"\\n\",\"color\":\"none\"},{\"text\":\"Winning Score:\",\"color\":\"none\"},{\"text\":\" \",\"color\":\"none\"},{\"text\":\"[-]\",\"color\":\"red\",\"clickEvent\":{\"action\":\"run_command\",\"value\":\"/trigger CTFTrigger set 5\"}},{\"text\":\" \",\"color\":\"none\"},{\"text\":\"[+]\",\"color\":\"dark_green\",\"clickEvent\":{\"action\":\"run_command\",\"value\":\"/trigger CTFTrigger set 4\"}},{\"text\":\"\\n\",\"color\":\"none\"},{\"text\":\"Time Limit: \",\"color\":\"none\"},{\"text\":\"\\n\",\"color\":\"none\"},{\"text\":\"[-1 min]\",\"color\":\"red\",\"clickEvent\":{\"action\":\"run_command\",\"value\":\"/trigger CTFTrigger set 3\"}},{\"text\":\" \",\"color\":\"none\"},{\"text\":\"[+1 min]\",\"color\":\"dark_green\",\"clickEvent\":{\"action\":\"run_command\",\"value\":\"/trigger CTFTrigger set 2\"}}]"],title:Book,author:TellrawGenerator}
/minecraft:replaceitem entity @a[score_CTFActive_min=1,score_MCTeams_min=8,score_MCTeams=8] slot.hotbar.6 written_book 1 0 {pages:["[\"\",{\"text\":\"Capture the Flag\",\"bold\":true},{\"text\":\"\\n\",\"bold\":false},{\"text\":\"Game: \"},{\"text\":\"[Start]\",\"color\":\"dark_green\",\"clickEvent\":{\"action\":\"run_command\",\"value\":\"/trigger CTFTrigger set 1\"}},{\"text\":\"\\n\",\"color\":\"none\"},{\"text\":\"Teams: \",\"color\":\"none\"},{\"text\":\"\\n\",\"color\":\"none\"},{\"text\":\"[Add Red]\",\"color\":\"red\",\"clickEvent\":{\"action\":\"run_command\",\"value\":\"/trigger CTFTrigger set 6\"}},{\"text\":\" \",\"color\":\"none\"},{\"text\":\"[Add Blue]\",\"color\":\"blue\",\"clickEvent\":{\"action\":\"run_command\",\"value\":\"/trigger CTFTrigger set 7\"}},{\"text\":\"\\n\",\"color\":\"none\"},{\"text\":\"Winning Score:\",\"color\":\"none\"},{\"text\":\" \",\"color\":\"none\"},{\"text\":\"[-]\",\"color\":\"red\",\"clickEvent\":{\"action\":\"run_command\",\"value\":\"/trigger CTFTrigger set 5\"}},{\"text\":\" \",\"color\":\"none\"},{\"text\":\"[+]\",\"color\":\"dark_green\",\"clickEvent\":{\"action\":\"run_command\",\"value\":\"/trigger CTFTrigger set 4\"}},{\"text\":\"\\n\",\"color\":\"none\"},{\"text\":\"Time Limit: \",\"color\":\"none\"},{\"text\":\"\\n\",\"color\":\"none\"},{\"text\":\"[-1 min]\",\"color\":\"red\",\"clickEvent\":{\"action\":\"run_command\",\"value\":\"/trigger CTFTrigger set 3\"}},{\"text\":\" \",\"color\":\"none\"},{\"text\":\"[+1 min]\",\"color\":\"dark_green\",\"clickEvent\":{\"action\":\"run_command\",\"value\":\"/trigger CTFTrigger set 2\"}}]"],title:"CTF Control Panel",author:"Icohedron"}

// Trigger for Event Coordinators
/minecraft:scoreboard players enable @a[score_CTFActive_min=1,score_MCTeams_min=8,score_MCTeams=8] CTFTrigger
// Move from lobby state to start state
/minecraft:execute @a[score_CTFTrigger_min=1,score_CTFTrigger=1] ~ ~ ~ minecraft:summon AreaEffectCloud ~ ~ ~ {Duration:1,Tags:["CTFStartGame"]}
// Increment/Decrement time by 1 minute
/minecraft:execute @a[score_CTFTrigger_min=2,score_CTFTrigger=2] ~ ~ ~ minecraft:scoreboard players add TimeInTicks CTFRoundTime 1200
/minecraft:execute @a[score_CTFTrigger_min=3,score_CTFTrigger=3] ~ ~ ~ minecraft:scoreboard players remove TimeInTicks CTFRoundTime 1200
// Increment/Decrement winning score by 1
/minecraft:execute @a[score_CTFTrigger_min=4,score_CTFTrigger=4] ~ ~ ~ minecraft:scoreboard players add Winning_Score CTFScore 1
/minecraft:execute @a[score_CTFTrigger_min=5,score_CTFTrigger=5] ~ ~ ~ minecraft:scoreboard players remove Winning_Score CTFScore 1
// Manual team balancing
/minecraft:execute @a[score_CTFTrigger_min=6,score_CTFTrigger=6] ~ ~ ~ minecraft:scoreboard players tag @r[score_CTFActive_min=1,score_CTFActive=1,team=CTFBlue] add CTFJoinRed
/minecraft:execute @a[score_CTFTrigger_min=7,score_CTFTrigger=7] ~ ~ ~ minecraft:scoreboard players tag @r[score_CTFActive_min=1,score_CTFActive=1,team=CTFRed] add CTFJoinBlue
// Change time or winning score via scoreboard commands
// execute @a[score_CTFTrigger_min=8,score_CTFTrigger=8] ~ ~ ~ minecraft:tellraw @p ["",{"text":"Click ","color":"gold","bold":true},{"text":"here","color":"dark_aqua","bold":true,"clickEvent":{"action":"suggest_command","value":"/scoreboard players set TimeInTicks CTFRoundTime 12000"}},{"text":" to set the time limit","color":"gold","bold":true}]
// execute @a[score_CTFTrigger_min=9,score_CTFTrigger=9] ~ ~ ~ minecraft:tellraw @p ["",{"text":"Click ","color":"gold","bold":true},{"text":"here","color":"dark_aqua","bold":true,"clickEvent":{"action":"suggest_command","value":"/scoreboard players set Winning_Score CTFScore 3"}},{"text":" to set the winning score","color":"gold","bold":true}]
/minecraft:scoreboard players set @a[score_CTFTrigger_min=1] CTFTrigger 0

// Set the default game mode
/minecraft:gamemode adventure @a[score_CTFActive_min=1,score_CTFActive=1,m=!adventure]

// Time
/minecraft:scoreboard players operation Minutes CTFRoundTime = @e[type=ArmorStand,tag=CTFGame] CTFRoundTime
/minecraft:scoreboard players operation Minutes CTFRoundTime /= TicksPerMinute CTFRoundTime
/minecraft:scoreboard players operation Minutes CTFScore = Minutes CTFRoundTime

/minecraft:scoreboard players operation Seconds CTFRoundTime = @e[type=ArmorStand,tag=CTFGame] CTFRoundTime
/minecraft:scoreboard players operation Seconds CTFRoundTime %= TicksPerMinute CTFRoundTime
/minecraft:scoreboard players operation Seconds CTFRoundTime /= TicksPerSecond CTFRoundTime
/minecraft:scoreboard players operation Seconds CTFScore = Seconds CTFRoundTime

//## Assign Teams -- Tagging ###

// Reset team counter
/minecraft:scoreboard players set @e[type=ArmorStand,tag=CTFRedMarker] CTFTeamCounter 0
/minecraft:scoreboard players set @e[type=ArmorStand,tag=CTFBlueMarker] CTFTeamCounter 0
/minecraft:scoreboard players set @e[type=ArmorStand,tag=CTFTeamBalancer] CTFTeamCounter 0

// Count up the number of players on each team
/minecraft:execute @a[score_CTFActive_min=1,score_CTFActive=1,team=CTFRed] ~ ~ ~ minecraft:scoreboard players add @e[type=ArmorStand,tag=CTFRedMarker] CTFTeamCounter 1
/minecraft:execute @a[score_CTFActive_min=1,score_CTFActive=1,team=CTFBlue] ~ ~ ~ minecraft:scoreboard players add @e[type=ArmorStand,tag=CTFBlueMarker] CTFTeamCounter 1

// Ensure that players are not on other teams unrelated to CTF
/minecraft:scoreboard players tag @a[score_CTFActive_min=1,score_CTFActive=1,team=CTFRed] add CTFOnATeam
/minecraft:scoreboard players tag @a[score_CTFActive_min=1,score_CTFActive=1,team=CTFBlue] add CTFOnATeam
/minecraft:scoreboard teams leave @a[score_CTFActive_min=1,score_CTFActive=1,tag=!CTFOnATeam]
/minecraft:scoreboard players tag @a[tag=CTFOnATeam] remove CTFOnATeam

// Take the difference to determine which team needs the extra player (Red - Blue)
/minecraft:scoreboard players operation @e[type=ArmorStand,tag=CTFTeamBalancer] CTFTeamCounter = @e[type=ArmorStand,tag=CTFRedMarker] CTFTeamCounter
/minecraft:scoreboard players operation @e[type=ArmorStand,tag=CTFTeamBalancer] CTFTeamCounter -= @e[type=ArmorStand,tag=CTFBlueMarker] CTFTeamCounter

// Both teams have equal numbers of players. Join a random team
/minecraft:execute @e[type=ArmorStand,tag=CTFTeamBalancer,score_CTFTeamCounter_min=0,score_CTFTeamCounter=0] ~ ~ ~ minecraft:scoreboard players tag @r[score_CTFActive_min=1,score_CTFActive=1,team=,tag=!CTFJoiningTeam] add CTFJoinRandom
/minecraft:scoreboard players tag @e[tag=CTFRandomTeamMarker] remove CTFRandomTeamMarker
/minecraft:scoreboard players tag @r[type=ArmorStand,tag=CTFTeamMarker] add CTFRandomTeamMarker
/minecraft:execute @e[type=ArmorStand,tag=CTFRandomTeamMarker] ~ ~ ~ minecraft:execute @e[r=0,c=1,tag=CTFRedMarker] ~ ~ ~ minecraft:scoreboard players tag @p[tag=CTFJoinRandom] add CTFJoinRed
/minecraft:execute @e[type=ArmorStand,tag=CTFRandomTeamMarker] ~ ~ ~ minecraft:execute @e[r=0,c=1,tag=CTFBlueMarker] ~ ~ ~ minecraft:scoreboard players tag @p[tag=CTFJoinRandom] add CTFJoinBlue
/minecraft:scoreboard players tag @a[tag=CTFJoinRandom] remove CTFJoinRandom
/minecraft:scoreboard players tag @e[tag=CTFRandomTeamMarker] remove CTFRandomTeamMarker

/minecraft:scoreboard players tag @a[tag=CTFJoinRed] add CTFJoiningTeam
/minecraft:scoreboard players tag @a[tag=CTFJoinBlue] add CTFJoiningTeam

// Blue team has more players. Join Red
/minecraft:execute @e[type=ArmorStand,tag=CTFTeamBalancer,score_CTFTeamCounter=-1] ~ ~ ~ minecraft:scoreboard players tag @r[score_CTFActive_min=1,score_CTFActive=1,team=,tag=!CTFJoiningTeam] add CTFJoinRed
/minecraft:scoreboard players tag @a[tag=CTFJoinRed] add CTFJoiningTeam

// Red team has more players. Join Blue
/minecraft:execute @e[type=ArmorStand,tag=CTFTeamBalancer,score_CTFTeamCounter_min=1] ~ ~ ~ minecraft:scoreboard players tag @r[score_CTFActive_min=1,score_CTFActive=1,team=,tag=!CTFJoiningTeam] add CTFJoinBlue
/minecraft:scoreboard players tag @a[tag=CTFJoinBlue] add CTFJoiningTeam

// Recursive team assignment tagging to assign teams in a single tick
// function icohedron:ctf/assign_teams if @a[score_CTFActive_min=1,score_CTFActive=1,team=,tag=!CTFJoiningTeam]

//## End Team Assignment tagging ###

//## Team Assignment ###

// Leave the current team, set spawnpoint
/minecraft:scoreboard teams leave @a[tag=CTFJoiningTeam]
/minecraft:execute @a[tag=CTFJoiningTeam] ~ ~ ~ minecraft:tp @p @r[score_CTFActive_min=1,type=ArmorStand,tag=CTFLobbySpawnPoint,c=1]

// [User Input]
// Use regular spawn point system
// /minecraft:spawnpoint @a[tag=CTFJoiningTeam]
// /minecraft:execute @a[tag=CTFJoiningTeam] ~ ~ ~ minecraft:tellraw @p ["Your spawn point has been set"]

// Clear inventory, teleport to team spawn, announce joining of team, join team, give items
/minecraft:scoreboard players set @a[tag=CTFJoiningTeam,score_CTFFlagTracker_min=1] CTFFlagTracker 0
/minecraft:clear @a[tag=CTFJoiningTeam]

/minecraft:execute @a[tag=CTFJoinRed] ~ ~ ~ minecraft:tellraw @a[score_CTFActive_min=1] [{"selector":"@p[tag=CTFJoinRed]"}, " joined team ", {"text":"Red","color":"red"}, "!"]
/minecraft:scoreboard teams join CTFRed @a[tag=CTFJoinRed]
/minecraft:scoreboard players tag @a[tag=CTFJoinRed] remove CTFJoinRed

/minecraft:execute @a[tag=CTFJoinBlue] ~ ~ ~ minecraft:tellraw @a[score_CTFActive_min=1] [{"selector":"@p[tag=CTFJoinBlue]"}, " joined team ", {"text":"Blue","color":"blue"}, "!"]
/minecraft:scoreboard teams join CTFBlue @a[tag=CTFJoinBlue]
/minecraft:scoreboard players tag @a[tag=CTFJoinBlue] remove CTFJoinBlue

/minecraft:scoreboard players tag @a[tag=CTFJoiningTeam] remove CTFJoiningTeam

//## End Team Assignment ###

// Give armor
/minecraft:execute @a[score_CTFActive_min=1,score_CTFActive=1,team=CTFRed] ~ ~ ~ minecraft:replaceitem entity @p slot.armor.head leather_helmet 1 0 {display:{color:16711680},Unbreakable:1}
/minecraft:execute @a[score_CTFActive_min=1,score_CTFActive=1,team=CTFRed] ~ ~ ~ minecraft:replaceitem entity @p slot.armor.chest leather_chestplate 1 0 {display:{color:16711680},Unbreakable:1}
/minecraft:execute @a[score_CTFActive_min=1,score_CTFActive=1,team=CTFRed] ~ ~ ~ minecraft:replaceitem entity @p slot.armor.legs leather_leggings 1 0 {display:{color:16711680},Unbreakable:1}
/minecraft:execute @a[score_CTFActive_min=1,score_CTFActive=1,team=CTFRed] ~ ~ ~ minecraft:replaceitem entity @p slot.armor.feet leather_boots 1 0 {display:{color:16711680},Unbreakable:1}

/minecraft:execute @a[score_CTFActive_min=1,score_CTFActive=1,team=CTFBlue] ~ ~ ~ minecraft:replaceitem entity @p slot.armor.head leather_helmet 1 0 {display:{color:255},Unbreakable:1}
/minecraft:execute @a[score_CTFActive_min=1,score_CTFActive=1,team=CTFBlue] ~ ~ ~ minecraft:replaceitem entity @p slot.armor.chest leather_chestplate 1 0 {display:{color:255},Unbreakable:1}
/minecraft:execute @a[score_CTFActive_min=1,score_CTFActive=1,team=CTFBlue] ~ ~ ~ minecraft:replaceitem entity @p slot.armor.legs leather_leggings 1 0 {display:{color:255},Unbreakable:1}
/minecraft:execute @a[score_CTFActive_min=1,score_CTFActive=1,team=CTFBlue] ~ ~ ~ minecraft:replaceitem entity @p slot.armor.feet leather_boots 1 0 {display:{color:255},Unbreakable:1}

// Spectators
/minecraft:scoreboard players enable @a[score_CTFActive_min=1] CTFSpectate
// Assign score of 1 for adding a spectator
// Assign score of -1 for removing a spectator
// Score of 2 means they are already in spectate
// Score of 0 means they are not spectating
/minecraft:execute @a[score_CTFSpectate_min=1,score_CTFSpectate=1] ~ ~ ~ minecraft:tp @p @r[score_CTFActive_min=1,type=ArmorStand,tag=CTFSpectatorSpawnPoint]
/minecraft:clear @a[score_CTFSpectate_min=1,score_CTFSpectate=1]
/minecraft:scoreboard players set @a[score_CTFSpectate_min=1,score_CTFSpectate=1] CTFSpectate 2

/minecraft:execute @a[score_CTFSpectate_min=-1,score_CTFSpectate=-1] ~ ~ ~ minecraft:tp @p @r[score_CTFActive_min=1,type=ArmorStand,tag=CTFLobbySpawnPoint]
/minecraft:scoreboard players set @a[score_CTFSpectate_min=-1,score_CTFSpectate=-1] CTFSpectate 0

// Spectator effects
/minecraft:effect @a[score_CTFSpectate_min=1] weakness 1 100 true
/minecraft:effect @a[score_CTFSpectate_min=1] instant_health 1 2 true
/minecraft:effect @a[score_CTFSpectate_min=1] saturation 1 2 true

:lobby CLOCK 16 0 1
// This condition must pass before the rest of the commands in this file should run.
/minecraft:testfor @e[type=ArmorStand,tag=CTFGame,score_CTFGameState_min=0,score_CTFGameState=0]
// For command block chains initially facing towards positive x (don't set to "Always Active", must be turned on/off with the condition above. Otherwise, it may keep running even after the arena is turned off):
/minecraft:setblock ~ ~ ~ minecraft:repeating_command_block 13 replace {TrackOutput:0b,auto:0b}

// Effects
/minecraft:effect @a[score_CTFActive_min=1,score_CTFActive=1] weakness 1 100 true
/minecraft:effect @a[score_CTFActive_min=1,score_CTFActive=1] instant_health 1 2 true
/minecraft:effect @a[score_CTFActive_min=1,score_CTFActive=1] saturation 1 2 true

// Kill items and xp
/minecraft:kill @e[score_CTFActive_min=1,type=Item]
/minecraft:kill @e[score_CTFActive_min=1,type=XPOrb]
/minecraft:kill @e[score_CTFActive_min=1,type=Arrow]

// Clear arrows
/minecraft:execute @e[type=ArmorStand,tag=CTFGame,score_CTFGameState_min=0,score_CTFGameState=0] ~ ~ ~ minecraft:clear @e[score_CTFActive_min=1,score_CTFActive=1] arrow

// Synchronize the timer while it is still able to be configured
/minecraft:scoreboard players operation @e[type=ArmorStand,tag=CTFGame] CTFRoundTime = TimeInTicks CTFRoundTime

// Switch to game state 1 and teleport players to a random spawn point of their team. Give players their items
/minecraft:execute @e[type=AreaEffectCloud,tag=CTFStartGame] ~ ~ ~ minecraft:scoreboard players set @e[type=ArmorStand,tag=CTFGame] CTFGameState 1
/minecraft:execute @e[type=AreaEffectCloud,tag=CTFStartGame] ~ ~ ~ minecraft:execute @a[score_CTFActive_min=1,score_CTFActive=1,team=CTFRed] ~ ~ ~ minecraft:tp @p @r[score_CTFActive_min=1,type=ArmorStand,tag=CTFRedSpawnPoint]
