// If the flag marker isn't holding the flag, a player must be. So kill existing flags
/minecraft:execute @e[score_CTFActive_min=1,type=ArmorStand,tag=CTFRedFlagMarker,score_CTFFlagTracker=0] ~ ~ ~ minecraft:kill @e[type=Item,tag=CTFRedFlag]
/minecraft:execute @e[score_CTFActive_min=1,type=ArmorStand,tag=CTFBlueFlagMarker,score_CTFFlagTracker=0] ~ ~ ~ minecraft:kill @e[type=Item,tag=CTFBlueFlag]

// Summon a flag if the marker is holding it. Don't summon if one already exists
/minecraft:execute @e[type=ArmorStand,tag=CTFExists] ~ ~ ~ minecraft:execute @e[type=Item,tag=CTFRedFlag] ~ ~ ~ minecraft:scoreboard players set @e[type=ArmorStand,tag=CTFExists] CTFExists 1
/minecraft:execute @e[type=ArmorStand,tag=CTFExists,score_CTFExists=0] ~ ~ ~ minecraft:execute @e[score_CTFActive_min=1,type=ArmorStand,tag=CTFRedFlagMarker,score_CTFFlagTracker_min=1] ~ ~ ~ minecraft:summon Item ~ ~ ~ {Tags:["CTFRedFlag","CTFFlag"],PickupDelay:32767,Age:-32768,Item:{id:"wool",Count:1,Damage:14},CustomName:"Red Flag",CustomNameVisible:true,Glowing:true}
/minecraft:execute @e[type=ArmorStand,tag=CTFExists,score_CTFExists=0] ~ ~ ~ minecraft:execute @e[score_CTFActive_min=1,type=ArmorStand,tag=CTFRedFlagMarker,score_CTFFlagTracker_min=1] ~ ~ ~ minecraft:scoreboard teams join CTFVRed @e[type=Item,tag=CTFRedFlag]
/minecraft:scoreboard players set @e[type=ArmorStand,tag=CTFExists] CTFExists 0

/minecraft:execute @e[type=ArmorStand,tag=CTFExists] ~ ~ ~ minecraft:execute @e[type=Item,tag=CTFBlueFlag] ~ ~ ~ minecraft:scoreboard players set @e[type=ArmorStand,tag=CTFExists] CTFExists 1
/minecraft:execute @e[type=ArmorStand,tag=CTFExists,score_CTFExists=0] ~ ~ ~ minecraft:execute @e[score_CTFActive_min=1,type=ArmorStand,tag=CTFBlueFlagMarker,score_CTFFlagTracker_min=1] ~ ~ ~ minecraft:summon Item ~ ~ ~ {Tags:["CTFBlueFlag","CTFFlag"],PickupDelay:32767,Age:-32768,Item:{id:"wool",Count:1,Damage:11},CustomName:"Blue Flag",CustomNameVisible:true,Glowing:true}
/minecraft:execute @e[type=ArmorStand,tag=CTFExists,score_CTFExists=0] ~ ~ ~ minecraft:execute @e[score_CTFActive_min=1,type=ArmorStand,tag=CTFBlueFlagMarker,score_CTFFlagTracker_min=1] ~ ~ ~ minecraft:scoreboard teams join CTFVBlue @e[type=Item,tag=CTFBlueFlag]
/minecraft:scoreboard players set @e[type=ArmorStand,tag=CTFExists] CTFExists 0

// Expose the thief!
/minecraft:effect @a[score_CTFActive_min=1,score_CTFFlagTracker_min=1] glowing 1 0 false

// Flag drop expiration
/minecraft:scoreboard players tag @e[type=AreaEffectCloud,tag=CTFRedFlagTimer] add CTFKillRedFlag {Age:1200}
/minecraft:scoreboard players tag @e[type=AreaEffectCloud,tag=CTFBlueFlagTimer] add CTFKillBlueFlag {Age:1200}

/minecraft:execute @e[type=AreaEffectCloud,tag=CTFKillRedFlag] ~ ~ ~ minecraft:tellraw @a[score_CTFActive_min=1] ["The ", {"text":"Red","color":"red"}, " flag has been returned to its base!"]
/minecraft:execute @e[type=AreaEffectCloud,tag=CTFKillRedFlag] ~ ~ ~ minecraft:execute @a[score_CTFActive_min=1] ~ ~ ~ minecraft:playsound block.note.hat master @p ~ ~ ~
/minecraft:execute @e[type=AreaEffectCloud,tag=CTFKillBlueFlag] ~ ~ ~ minecraft:tellraw @a[score_CTFActive_min=1] ["The ", {"text":"Blue","color":"blue"}, " flag has been returned to its base!"]
/minecraft:execute @e[type=AreaEffectCloud,tag=CTFKillBlueFlag] ~ ~ ~ minecraft:execute @a[score_CTFActive_min=1] ~ ~ ~ minecraft:playsound block.note.hat master @p ~ ~ ~

/minecraft:execute @e[type=AreaEffectCloud,tag=CTFKillRedFlag] ~ ~ ~ minecraft:kill @e[type=Item,tag=CTFRedFlag]
/minecraft:execute @e[type=AreaEffectCloud,tag=CTFKillBlueFlag] ~ ~ ~ minecraft:kill @e[type=Item,tag=CTFBlueFlag]

/minecraft:scoreboard players tag @e[type=AreaEffectCloud,tag=CTFRedFlagTimer] remove CTFKillRedFlag
/minecraft:scoreboard players tag @e[type=AreaEffectCloud,tag=CTFBlueFlagTimer] remove CTFKillBlueFlag

// Check if flag has been captured
/minecraft:scoreboard players add @e[type=ArmorStand,tag=CTFRedMarker] CTFFlagTracker 0
/minecraft:scoreboard players add @e[type=ArmorStand,tag=CTFBlueMarker] CTFFlagTracker 0

/minecraft:execute @e[score_CTFActive_min=1,type=ArmorStand,tag=CTFRedFlagMarker] ~ ~ ~ minecraft:scoreboard players tag @p[score_CTFFlagTracker_min=1,team=CTFRed,r=1] add CTFScoreRed
// Kill flag trackers
/minecraft:execute @a[tag=CTFScoreRed] ~ ~ ~ minecraft:kill @e[type=AreaEffectCloud,tag=CTFBlueFlagTracker]
// Let go of the flag
/minecraft:scoreboard players set @a[tag=CTFScoreRed] CTFFlagTracker 0
// Announce score
/minecraft:execute @a[tag=CTFScoreRed] ~ ~ ~ minecraft:tellraw @a[score_CTFActive_min=1] [{"selector":"@p"}, " has successfully captured the ", {"text":"Blue","color":"blue"}, " flag!"]
/minecraft:execute @a[tag=CTFScoreRed] ~ ~ ~ minecraft:execute @a[score_CTFActive_min=1] ~ ~ ~ minecraft:playsound entity.player.levelup master @p ~ ~ ~
// Remove glowing effect
/minecraft:effect @a[tag=CTFScoreRed] glowing 0
// Add 1 to red's score
/minecraft:execute @a[tag=CTFScoreRed] ~ ~ ~ minecraft:scoreboard players add @e[type=ArmorStand,tag=CTFRedMarker] CTFFlagTracker 1
/minecraft:scoreboard players tag @a[tag=CTFScoreRed] remove CTFScoreRed

// Rinse and repeat for blue team
/minecraft:execute @e[score_CTFActive_min=1,type=ArmorStand,tag=CTFBlueFlagMarker] ~ ~ ~ minecraft:scoreboard players tag @p[score_CTFFlagTracker_min=1,team=CTFBlue,r=1] add CTFScoreBlue
/minecraft:execute @a[tag=CTFScoreBlue] ~ ~ ~ minecraft:kill @e[type=AreaEffectCloud,tag=CTFRedFlagTracker]
/minecraft:scoreboard players set @a[tag=CTFScoreBlue] CTFFlagTracker 0
/minecraft:execute @a[tag=CTFScoreBlue] ~ ~ ~ minecraft:tellraw @a[score_CTFActive_min=1] [{"selector":"@p"}, " has successfully captured the ", {"text":"Red","color":"red"}, " flag!"]
/minecraft:execute @a[tag=CTFScoreBlue] ~ ~ ~ minecraft:execute @a[score_CTFActive_min=1] ~ ~ ~ minecraft:playsound entity.player.levelup master @p ~ ~ ~
/minecraft:effect @a[tag=CTFScoreBlue] glowing 0
/minecraft:execute @a[tag=CTFScoreBlue] ~ ~ ~ minecraft:scoreboard players add @e[type=ArmorStand,tag=CTFBlueMarker] CTFFlagTracker 1
/minecraft:scoreboard players tag @a[tag=CTFScoreBlue] remove CTFScoreBlue

// Update Score scoreboard
/minecraft:scoreboard players operation Red CTFScore = @e[type=ArmorStand,tag=CTFRedMarker] CTFFlagTracker
/minecraft:scoreboard players operation Blue CTFScore = @e[type=ArmorStand,tag=CTFBlueMarker] CTFFlagTracker

// Check if red or blue have reached the winning score
/minecraft:scoreboard players operation @e[type=ArmorStand,tag=CTFRedMarker] CTFScoreKeeper = @e[type=ArmorStand,tag=CTFRedMarker] CTFFlagTracker
/minecraft:scoreboard players operation @e[type=ArmorStand,tag=CTFRedMarker] CTFScoreKeeper -= Winning_Score CTFScore
/minecraft:scoreboard players operation @e[type=ArmorStand,tag=CTFBlueMarker] CTFScoreKeeper = @e[type=ArmorStand,tag=CTFBlueMarker] CTFFlagTracker
/minecraft:scoreboard players operation @e[type=ArmorStand,tag=CTFBlueMarker] CTFScoreKeeper -= Winning_Score CTFScore
// Change to game state 3 if the winning score has been reached
/minecraft:execute @e[type=ArmorStand,tag=CTFRedMarker,score_CTFScoreKeeper_min=0] ~ ~ ~ minecraft:scoreboard players set @e[type=ArmorStand,tag=CTFGame] CTFGameState 3
/minecraft:execute @e[type=ArmorStand,tag=CTFBlueMarker,score_CTFScoreKeeper_min=0] ~ ~ ~ minecraft:scoreboard players set @e[type=ArmorStand,tag=CTFGame] CTFGameState 3

// Change to game state 3 if the time limit has been reached
/minecraft:scoreboard players set @e[type=ArmorStand,tag=CTFGame,score_CTFRoundTime=0] CTFGameState 3
/minecraft:execute @e[type=ArmorStand,tag=CTFGame,score_CTFRoundTime_min=0,score_CTFRoundTime=0] ~ ~ ~ minecraft:tellraw @a[score_CTFActive_min=1] ["Time is up!"]

// Reset death counter
/minecraft:scoreboard players set @a CTFDeathCount 0

// Count down on timer
/minecraft:scoreboard players remove @e[type=ArmorStand,tag=CTFGame] CTFRoundTime 1

:end CLOCK 16 0 1
// This condition must pass before the rest of the commands in this file should run.
/minecraft:testfor @e[type=ArmorStand,tag=CTFGame,score_CTFGameState_min=3,score_CTFGameState=3]
// For command block chains initially facing towards positive x (don't set to "Always Active", must be turned on/off with the condition above. Otherwise, it may keep running even after the arena is turned off):
/minecraft:setblock ~ ~ ~ minecraft:repeating_command_block 13 replace {TrackOutput:0b,auto:0b}

// Effects
/minecraft:effect @a[score_CTFActive_min=1,score_CTFActive=1] saturation 1 2 true
/minecraft:effect @a[score_CTFActive_min=1,score_CTFActive=1] weakness 1 100 true
/minecraft:effect @a[score_CTFActive_min=1,score_CTFActive=1] instant_health 1 2 true

// Kill items and xp
/minecraft:kill @e[score_CTFActive_min=1,type=Item,tag=!CTFFlag]
/minecraft:kill @e[score_CTFActive_min=1,type=XPOrb]
/minecraft:kill @e[score_CTFActive_min=1,type=Arrow]
// Remove flags as to not give players the false message that they stil have it
/minecraft:replaceitem entity @a[score_CTFActive_min=1,score_CTFActive=1] slot.hotbar.4 air 1 0

// Summon end countdown timer if it doesn't exist
/minecraft:execute @e[type=ArmorStand,tag=CTFExists] ~ ~ ~ minecraft:execute @e[type=AreaEffectCloud,tag=CTFEndTimer] ~ ~ ~ minecraft:scoreboard players set @e[type=ArmorStand,tag=CTFExists] CTFExists 1
/minecraft:execute @e[type=ArmorStand,tag=CTFExists,score_CTFExists=0] ~ ~ ~ minecraft:summon AreaEffectCloud ~ ~ ~ {Duration:201,Tags:["CTFEndTimer"]}
/minecraft:scoreboard players set @e[type=ArmorStand,tag=CTFExists] CTFExists 0

// Add tags to fireworks and area effect clouds according to their age/life
/minecraft:scoreboard players tag @e[type=AreaEffectCloud,tag=CTFEndTimer] add CTFInitialRocket {Age:0}
/minecraft:scoreboard players tag @e[type=FireworksRocketEntity,tag=CTFFireworkTimer,c=1] add CTFCreateRocket {Life:20}
/minecraft:scoreboard players tag @e[type=AreaEffectCloud,tag=CTFEndTimer] add CTFEndGame {Age:200}

// Determine the winner based on score
// Take the difference in score (Red - Blue) to determine the winning team
/minecraft:execute @e[type=AreaEffectCloud,tag=CTFInitialRocket] ~ ~ ~ minecraft:scoreboard players operation @e[type=ArmorStand,tag=CTFGame] CTFScoreKeeper = Red CTFScore
/minecraft:execute @e[type=AreaEffectCloud,tag=CTFInitialRocket] ~ ~ ~ minecraft:scoreboard players operation @e[type=ArmorStand,tag=CTFGame] CTFScoreKeeper -= Blue CTFScore
// Summon the initial firework for the winning team
/minecraft:execute @e[type=AreaEffectCloud,tag=CTFInitialRocket] ~ ~ ~ minecraft:execute @e[type=ArmorStand,tag=CTFGame,score_CTFScoreKeeper_min=1] ~ ~ ~ minecraft:execute @a[score_CTFActive_min=1,score_CTFActive=1,team=CTFRed] ~ ~ ~ minecraft:summon FireworksRocketEntity ~ ~3 ~ {LifeTime:21,Tags:["CTFFireworkTimer"]}
/minecraft:execute @e[type=AreaEffectCloud,tag=CTFInitialRocket] ~ ~ ~ minecraft:execute @e[type=ArmorStand,tag=CTFGame,score_CTFScoreKeeper=-1] ~ ~ ~ minecraft:execute @a[score_CTFActive_min=1,score_CTFActive=1,team=CTFBlue] ~ ~ ~ minecraft:summon FireworksRocketEntity ~ ~3 ~ {LifeTime:21,Tags:["CTFFireworkTimer"]}
// Announce winners
/minecraft:execute @e[type=AreaEffectCloud,tag=CTFInitialRocket] ~ ~ ~ minecraft:execute @e[type=ArmorStand,tag=CTFGame,score_CTFScoreKeeper_min=0,score_CTFScoreKeeper=0] ~ ~ ~ minecraft:execute @e[type=ArmorStand,tag=CTFGame,score_CTFGameState_min=3] ~ ~ ~ minecraft:tellraw @a[score_CTFActive_min=1] ["Stalemate!"]
/minecraft:execute @e[type=AreaEffectCloud,tag=CTFInitialRocket] ~ ~ ~ minecraft:execute @e[type=ArmorStand,tag=CTFGame,score_CTFScoreKeeper_min=0,score_CTFScoreKeeper=0] ~ ~ ~ minecraft:execute @e[type=ArmorStand,tag=CTFGame,score_CTFGameState_min=3] ~ ~ ~ minecraft:title @a[score_CTFActive_min=1] subtitle ["Stalemate!"]
/minecraft:execute @e[type=AreaEffectCloud,tag=CTFInitialRocket] ~ ~ ~ minecraft:execute @e[type=ArmorStand,tag=CTFGame,score_CTFScoreKeeper_min=1] ~ ~ ~ minecraft:tellraw @a[score_CTFActive_min=1] ["", {"text":"Red","color":"red"}, " team has won!"]
/minecraft:execute @e[type=AreaEffectCloud,tag=CTFInitialRocket] ~ ~ ~ minecraft:execute @e[type=ArmorStand,tag=CTFGame,score_CTFScoreKeeper_min=1] ~ ~ ~ minecraft:title @a[score_CTFActive_min=1] subtitle ["", {"text":"Red","color":"red"}, " team has won!"]
/minecraft:execute @e[type=AreaEffectCloud,tag=CTFInitialRocket] ~ ~ ~ minecraft:execute @e[type=ArmorStand,tag=CTFGame,score_CTFScoreKeeper=-1] ~ ~ ~ minecraft:tellraw @a[score_CTFActive_min=1] ["", {"text":"Blue","color":"blue"}, " team has won!"]
/minecraft:execute @e[type=AreaEffectCloud,tag=CTFInitialRocket] ~ ~ ~ minecraft:execute @e[type=ArmorStand,tag=CTFGame,score_CTFScoreKeeper=-1] ~ ~ ~ minecraft:title @a[score_CTFActive_min=1] subtitle ["", {"text":"Blue","color":"blue"}, " team has won!"]
/minecraft:execute @e[type=AreaEffectCloud,tag=CTFInitialRocket] ~ ~ ~ minecraft:title @a[score_CTFActive_min=1] title [""]
// List players on winning team to event coordinators
/minecraft:execute @e[type=AreaEffectCloud,tag=CTFInitialRocket] ~ ~ ~ minecraft:execute @e[type=ArmorStand,tag=CTFGame,score_CTFScoreKeeper_min=1] ~ ~ ~ minecraft:tellraw @a[score_CTFActive_min=1,score_MCTeams_min=8,score_MCTeams=8] ["Players on winning team: ", {"selector":"@a[score_CTFActive_min=1,score_CTFActive=1,team=CTFRed]"}]
/minecraft:execute @e[type=AreaEffectCloud,tag=CTFInitialRocket] ~ ~ ~ minecraft:execute @e[type=ArmorStand,tag=CTFGame,score_CTFScoreKeeper=-1] ~ ~ ~ minecraft:tellraw @a[score_CTFActive_min=1,score_MCTeams_min=8,score_MCTeams=8] ["Players on winning team: ", {"selector":"@a[score_CTFActive_min=1,score_CTFActive=1,team=CTFBlue]"}]

// Summon another rocket if the existing ones have reached their time limit
/minecraft:execute @e[type=FireworksRocketEntity,tag=CTFCreateRocket] ~ ~ ~ minecraft:execute @e[type=ArmorStand,tag=CTFGame,score_CTFScoreKeeper_min=1] ~ ~ ~ minecraft:execute @a[score_CTFActive_min=1,score_CTFActive=1,team=CTFRed] ~ ~ ~ minecraft:summon FireworksRocketEntity ~ ~3 ~ {LifeTime:21,Tags:["CTFFireworkTimer"]}
/minecraft:execute @e[type=FireworksRocketEntity,tag=CTFCreateRocket] ~ ~ ~ minecraft:execute @e[type=ArmorStand,tag=CTFGame,score_CTFScoreKeeper=-1] ~ ~ ~ minecraft:execute @a[score_CTFActive_min=1,score_CTFActive=1,team=CTFBlue] ~ ~ ~ minecraft:summon FireworksRocketEntity ~ ~3 ~ {LifeTime:21,Tags:["CTFFireworkTimer"]}

// Reset the game and switch to game state 0
/minecraft:execute @e[type=AreaEffectCloud,tag=CTFEndGame] ~ ~ ~ minecraft:execute @a[score_CTFActive_min=1,score_CTFActive=1] ~ ~ ~ minecraft:tp @p @r[score_CTFActive_min=1,type=ArmorStand,tag=CTFLobbySpawnPoint]
/minecraft:execute @e[type=AreaEffectCloud,tag=CTFEndGame] ~ ~ ~ minecraft:scoreboard players operation @e[type=ArmorStand,tag=CTFGame] CTFRoundTime = TimeInTicks CTFRoundTime
/minecraft:execute @e[type=AreaEffectCloud,tag=CTFEndGame] ~ ~ ~ minecraft:scoreboard players set Red CTFScore 0
/minecraft:execute @e[type=AreaEffectCloud,tag=CTFEndGame] ~ ~ ~ minecraft:scoreboard players set Blue CTFScore 0
/minecraft:execute @e[type=AreaEffectCloud,tag=CTFEndGame] ~ ~ ~ minecraft:scoreboard players set * CTFFlagTracker 0
/minecraft:execute @e[type=AreaEffectCloud,tag=CTFEndGame] ~ ~ ~ minecraft:scoreboard players set @e[type=ArmorStand,tag=CTFGame] CTFGameState 0

// Remove the tags
/minecraft:scoreboard players tag @e[type=AreaEffectCloud,tag=CTFInitialRocket] remove CTFInitialRocket
/minecraft:scoreboard players tag @e[type=FireworksRocketEntity,tag=CTFCreateRocket] remove CTFCreateRocket
/minecraft:scoreboard players tag @e[type=AreaEffectCloud,tag=CTFEndGame] remove CTFEndGame
